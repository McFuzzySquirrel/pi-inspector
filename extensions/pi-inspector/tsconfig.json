{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "outDir": "dist",
  "lib": ["ES2022"],
  "moduleResolution": "node",
  "types": ["node", "vscode"],
  "typeRoots": ["./node_modules/@types"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
